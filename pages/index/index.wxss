.sliders{
	height: 380rpx;
}
.sliders navigator{
	width: 100%;
	height: 100%;
}
.sliders image{
	width: 100%;
	height: 100%;
}
.grids{
	display: flex;
	flex-wrap: wrap;
	font-size: 28rpx;
	background-color: #fff;
	border-top: 1rpx solid #eee;
	border-left: 1rpx solid #eee;
	font-size: 28rpx;

}
.grids .item{
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	width: 33.3333333%;
	height: 250rpx;
	border-right: 1rpx solid #eee;
	border-bottom: 1rpx solid #eee;
	box-sizing: border-box;
}
.grids .item image{
	width: 70rpx;
	height: 70rpx;
}

.grids .item text{
	margin-top: 20rpx;
	display: block;
	color: #888;
	font-size: 28rpx;
}